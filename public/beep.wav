function NotificationSettings({ isPremium }) {
  const [notifications, setNotifications] = useState([]);
  const [newTime, setNewTime] = useState("");
  const [newType, setNewType] = useState("薬");

  const maxNotifications = isPremium ? Infinity : 3;

  // 🔔 通知許可 &amp; タイマーで毎分チェック
  useEffect(() =&gt; {
    if ("Notification" in window &amp;&amp; Notification.permission !== "granted") {
      Notification.requestPermission();
    }

    const timer = setInterval(() =&gt; {
      const now = new Date();
      const currentTime = now.toTimeString().slice(0, 5); // "HH:MM"

      notifications.forEach(({ time, type }) =&gt; {
        if (time === currentTime &amp;&amp; Notification.permission === "granted") {
          const message = <code>${type}の時間です！</code>;
          new Notification(message);

          // 🔊 音を鳴らす
          const audio = new Audio("https://www.soundjay.com/button/sounds/beep-01a.wav");
          audio.play();

        }
      });
    }, 60000); // 1分ごと

    return () =&gt; clearInterval(timer);
  }, [notifications]);

  const handleAddNotification = () =&gt; {
    if (!newTime || notifications.length &gt;= maxNotifications) return;

    const newNotification = {
  id: <code>${newType}-${newTime}-${Date.now()}</code>, // ← ← ← ここを修正！
  time: newTime,
  type: newType
};

    setNotifications([...notifications, newNotification]);
    setNewTime("");
  };

  const handleDelete = (id) =&gt; {
    setNotifications(notifications.filter(n =&gt; n.id !== id));
  };

  return (
    &lt;div style={{
      marginTop: 30,
      padding: 20,
      border: "2px solid #4CAF50",
      borderRadius: 12,
      background: "#f9fff9"
    }}&gt;
      &lt;h3 style={{ marginBottom: 10 }}&gt;🔔 通知設定&lt;/h3&gt;
      &lt;div style={{ marginBottom: 10 }}&gt;
        &lt;label&gt;種類：&lt;/label&gt;
        &lt;select value={newType} onChange={(e) =&gt; setNewType(e.target.value)}&gt;
          &lt;option value="薬"&gt;薬&lt;/option&gt;
          &lt;option value="運動"&gt;運動&lt;/option&gt;
          &lt;option value="食事"&gt;食事&lt;/option&gt;
        &lt;/select&gt;

        &lt;label style={{ marginLeft: 10 }}&gt;時間：&lt;/label&gt;
        &lt;input
          type="time"
          value={newTime}
          onChange={(e) =&gt; setNewTime(e.target.value)}
        /&gt;

        &lt;button
          style={{ marginLeft: 10, padding: "4px 10px" }}
          onClick={handleAddNotification}
          disabled={notifications.length &gt;= maxNotifications}
        &gt;
          登録
        &lt;/button&gt;

        {!isPremium &amp;&amp; (
          &lt;p style={{ color: "gray", fontSize: "0.9em", marginTop: 5 }}&gt;
            ※無料版では1日最大3件まで登録できます
          &lt;/p&gt;
        )}
      &lt;/div&gt;

      &lt;ul style={{ listStyle: "none", padding: 0 }}&gt;
        {notifications.map((n) =&gt; (
          &lt;li key={n.id} style={{
            padding: "6px 10px",
            borderBottom: "1px solid #ccc",
            display: "flex",
            justifyContent: "space-between"
          }}&gt;
            &lt;span&gt;🕒 {n.time} - {n.type}&lt;/span&gt;
            &lt;button onClick={() =&gt; handleDelete(n.id)}&gt;削除&lt;/button&gt;
          &lt;/li&gt;
        ))}
      &lt;/ul&gt;
    &lt;/div&gt;
  );
}

export default NotificationSettings;</div></div></div></div><div class="text-token-text-secondary my-2 me-4 flex flex-row justify-end gap-1 text-xs"></div></div><div class="flex justify-end"><div class="touch:-me-2 touch:-ms-3.5 -ms-2.5 -me-1 flex flex-wrap items-center gap-y-4 p-1 select-none focus-within:transition-none hover:transition-none duration-300 group-hover/turn-messages:delay-300 pointer-events-none opacity-0 motion-safe:transition-opacity group-hover/turn-messages:pointer-events-auto group-hover/turn-messages:opacity-100 group-focus-within/turn-messages:pointer-events-auto group-focus-within/turn-messages:opacity-100 has-data-[state=open]:pointer-events-auto has-data-[state=open]:opacity-100" style="mask-position: 0% 0%;"><button class="text-token-text-secondary hover:bg-token-bg-secondary rounded-lg" aria-label="コピーする" aria-selected="false" data-testid="copy-turn-action-button" data-state="closed"><span class="touch:w-10 flex h-8 w-8 items-center justify-center"><svg width="20" height="20" viewBox="0 0 20 20" fill="currentColor" xmlns="http://www.w3.org/2000/svg" class="icon"><path d="M12.668 10.667C12.668 9.95614 12.668 9.46258 12.6367 9.0791C12.6137 8.79732 12.5758 8.60761 12.5244 8.46387L12.4688 8.33399C12.3148 8.03193 12.0803 7.77885 11.793 7.60254L11.666 7.53125C11.508 7.45087 11.2963 7.39395 10.9209 7.36328C10.5374 7.33197 10.0439 7.33203 9.33301 7.33203H6.5C5.78896 7.33203 5.29563 7.33195 4.91211 7.36328C4.63016 7.38632 4.44065 7.42413 4.29688 7.47559L4.16699 7.53125C3.86488 7.68518 3.61186 7.9196 3.43555 8.20703L3.36524 8.33399C3.28478 8.49198 3.22795 8.70352 3.19727 9.0791C3.16595 9.46259 3.16504 9.95611 3.16504 10.667V13.5C3.16504 14.211 3.16593 14.7044 3.19727 15.0879C3.22797 15.4636 3.28473 15.675 3.36524 15.833L3.43555 15.959C3.61186 16.2466 3.86474 16.4807 4.16699 16.6348L4.29688 16.6914C4.44063 16.7428 4.63025 16.7797 4.91211 16.8027C5.29563 16.8341 5.78896 16.835 6.5 16.835H9.33301C10.0439 16.835 10.5374 16.8341 10.9209 16.8027C11.2965