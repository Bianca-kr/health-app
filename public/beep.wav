ent-margin:--spacing(6)] @[72rem]:[--thread-content-margin:--spacing(16)] px-(--thread-content-margin)"><div class="[--thread-content-max-width:32rem] @[34rem]:[--thread-content-max-width:40rem] @[64rem]:[--thread-content-max-width:48rem] mx-auto flex max-w-(--thread-content-max-width) flex-1 text-base gap-4 md:gap-5 lg:gap-6 group/turn-messages focus-visible:outline-hidden" tabindex="-1"><div class="group/conversation-turn relative flex w-full min-w-0 flex-col"><div class="relative flex-col gap-1 md:gap-3"><div class="flex max-w-full flex-col grow"><div data-message-author-role="user" data-message-id="54503fab-d32b-4aa5-a9b6-bc03d925c95b" dir="auto" class="min-h-8 text-message relative flex w-full flex-col items-end gap-2 text-start break-words whitespace-normal [.text-message+&amp;]:mt-5"><div class="flex w-full flex-col gap-1 empty:hidden items-end rtl:items-start"><div class="relative max-w-[var(--user-chat-width,70%)] bg-token-message-surface rounded-3xl px-5 py-2.5"><div class="whitespace-pre-wrap">どうすればいい？import React, { useState } from 'react'; 


function NotificationSettings({ isPremium }) {
  const [notifications, setNotifications] = useState([]);
  const [newTime, setNewTime] = useState("");
  const [newType, setNewType] = useState("薬");
  import { useEffect } from 'react'; // 👈 まだ書いてなければ、Reactの上に追加！

useEffect(() =&gt; {
  // ブラウザ通知の許可をリクエスト
  if ("Notification" in window &amp;&amp; Notification.permission !== "granted") {
    Notification.requestPermission();
  }

  const timer = setInterval(() =&gt; {
    const now = new Date();
    const currentTime = now.toTimeString().slice(0, 5); // "HH:MM"形式

    notifications.forEach(({ time, type }) =&gt; {
      if (time === currentTime &amp;&amp; Notification.permission === "granted") {
        new Notification(<code>${type}の時間です！</code>);
      }
    });
  }, 60000); // 1分ごとにチェック

  return () =&gt; clearInterval(timer); // コンポーネントが消えたときタイマー停止
}, [notifications]);


  const maxNotifications = isPremium ? Infinity : 3;

  const handleAddNotification = () =&gt; {
    if (!newTime || notifications.length &gt;= maxNotifications) return;

    const newNotification = { time: newTime, type: newType };
    setNotifications([...notifications, newNotification]);
    setNewTime("");
  };

  const handleDelete = (index) =&gt; {
    const updated = [...notifications];
    updated.splice(index, 1);
    setNotifications(updated);
  };

  return (
    &lt;div style={{ marginTop: 30, padding: 10, border: "1px solid #ccc", borderRadius: 8 }}&gt;
      &lt;h3&gt;通知設定&lt;/h3&gt;
      &lt;div&gt;
        &lt;label&gt;種類：&lt;/label&gt;
        &lt;select value={newType} onChange={(e) =&gt; setNewType(e.target.value)}&gt;
          &lt;option value="薬"&gt;薬&lt;/option&gt;
          &lt;option value="運動"&gt;運動&lt;/option&gt;
          &lt;option value="食事"&gt;食事&lt;/option&gt;
        &lt;/select&gt;

        &lt;label&gt;時間：&lt;/label&gt;
        &lt;input
          type="time"
          value={newTime}
          onChange={(e) =&gt; setNewTime(e.target.value)}
        /&gt;

        &lt;button onClick={handleAddNotification} disabled={notifications.length &gt;= maxNotifications}&gt;
          登録
        &lt;/button&gt;

        {!isPremium &amp;&amp; (
          &lt;p style={{ color: "gray" }}&gt;※無料版では1日最大3件まで登録できます&lt;/p&gt;
        )}
      &lt;/div&gt;

      &lt;ul&gt;
        {notifications.map((n, index) =&gt; (
          &lt;li key={index}&gt;
            [{n.type}] {n.time}
            &lt;button onClick={() =&gt; handleDelete(index)}&gt;削除&lt;/button&gt;
          &lt;/li&gt;
        ))}
      &lt;/ul&gt;
    &lt;/div&gt;
  );
}

