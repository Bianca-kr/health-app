          </span><span><span class="hljs-attr">value</span></span><span>=</span><span><span class="hljs-string">{newTime}</span></span><span>
          </span><span><span class="hljs-attr">onChange</span></span><span>=</span><span><span class="hljs-string">{(e)</span></span><span> =&gt; setNewTime(e.target.value)}
