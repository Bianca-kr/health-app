  }

  </span><span><span class="hljs-keyword">const</span></span><span> timer = </span><span><span class="hljs-built_in">setInterval</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> now = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Date</span></span><span>();
    </span><span><span class="hljs-keyword">const</span></span><span> currentTime = now.</span><span><span class="hljs-title function_">toTimeString</span></span><span>().</span><span><span class="hljs-title function_">slice</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">5</span></span><span>); </span><span><span class="hljs-comment">// "HH:MM"形式</span></span><span>

    notifications.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">{ time, type }</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">if</span></span><span> (time === currentTime &amp;&amp; </span><span><span class="hljs-title class_">Notification</span></span><span>.</span><span><span class="hljs-property">permission</span></span><span> === </span><span><span class="hljs-string">"granted"</span></span><span>) {
