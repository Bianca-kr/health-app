  </span><span><span class="hljs-keyword">const</span></span><span> maxNotifications = isPremium ? </span><span><span class="hljs-title class_">Infinity</span></span><span> : </span><span><span class="hljs-number">3</span></span><span>;

  </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">handleAddNotification</span></span><span> = (</span><span><span class="hljs-params"></span></span><span>) =&gt; {
