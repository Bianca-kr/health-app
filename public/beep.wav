        &lt;select value={newType} onChange={(e) =&gt; setNewType(e.target.value)}&gt;
          &lt;option value="è–¬"&gt;è–¬&lt;/option&gt;
          &lt;option value="é‹å‹•"&gt;é‹å‹•&lt;/option&gt;
          &lt;option value="é£Ÿäº‹"&gt;é£Ÿäº‹&lt;/option&gt;
        &lt;/select&gt;

        &lt;label&gt;æ™‚é–“ï¼š&lt;/label&gt;
        &lt;input
          type="time"
          value={newTime}
          onChange={(e) =&gt; setNewTime(e.target.value)}
        /&gt;

        &lt;button onClick={handleAddNotification} disabled={notifications.length &gt;= maxNotifications}&gt;
          ç™»éŒ²
        &lt;/button&gt;

        {!isPremium &amp;&amp; (
          &lt;p style={{ color: "gray" }}&gt;â€»ç„¡æ–™ç‰ˆã§ã¯1æ—¥æœ€å¤§3ä»¶ã¾ã§ç™»éŒ²ã§ãã¾ã™&lt;/p&gt;
